<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Xerago Digital Priority Properties"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
		            <column
		                jcr:primaryType="nt:unstructured"
		                sling:resourceType="granite/ui/components/coral/foundation/container">
		                <items jcr:primaryType="nt:unstructured">
		                	<digitalHeading jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
								fieldLabel="Heading"
				                fieldDescription="Enter heading for digital priority component"  name="./digitalHeading" />

		                    <column
				                jcr:primaryType="nt:unstructured"
				                sling:resourceType="granite/ui/components/coral/foundation/container">
				                <items jcr:primaryType="nt:unstructured">
				                    <digitalMultifield
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
										composite="{Boolean}true" fieldLabel="Sub List">
										<field jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/container"
											name="./digitalMultifield">
											<items jcr:primaryType="nt:unstructured">
												<innerImage
							                        jcr:primaryType="nt:unstructured"
							                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
							                        fieldLabel="Inner image"
							                        fieldDescription="Select inner image"
							                        name="./innerImage"/>
												<subHeading jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="Sub Heading"
				                                    fieldDescription="Enter sub heading"  name="./subHeading" />
				                                <description jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="Sub description"
				                                    fieldDescription="Enter sub heading"  name="./description" />
				                                
											</items>
										</field>
									</digitalMultifield>
								</items>
							</column>
		                </items>
		            </column>
        </items>
    </content>
</jcr:root>