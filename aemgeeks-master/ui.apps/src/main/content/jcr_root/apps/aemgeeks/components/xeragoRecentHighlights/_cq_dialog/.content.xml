<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Xerago Highlights  Properties"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
		            <column
		                jcr:primaryType="nt:unstructured"
		                sling:resourceType="granite/ui/components/coral/foundation/container">
		                <items jcr:primaryType="nt:unstructured">
		                    <highHeading jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
									fieldLabel="Title"
				                    fieldDescription="Enter Title of the component"  name="./highHeading" />
				            <highSubHeading jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
									fieldLabel="Sub Title"
				                    fieldDescription="Enter Sub-Title of the component"  name="./highSubHeading" />
		                    <column
				                jcr:primaryType="nt:unstructured"
				                sling:resourceType="granite/ui/components/coral/foundation/container">
				                <items jcr:primaryType="nt:unstructured">
				                    <highCards
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
										composite="{Boolean}true" fieldLabel="Sub cards">
										<field jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/container"
											name="./highCards">
											<items jcr:primaryType="nt:unstructured">
												<highCardTitle jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="Card Title"
				                                    fieldDescription="Enter card title "  name="./highCardTitle" />
				                                <highCardDes jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="Card Description"
				                                    fieldDescription="Enter card description "  name="./highCardDes" />
				                                <highMore jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="know more"
				                                    fieldDescription="Enter text"  name="./highMore" />
											</items>
										</field>
									</highCards>
								</items>
							</column>
		                </items>
		            </column>
        </items>
    </content>
</jcr:root>